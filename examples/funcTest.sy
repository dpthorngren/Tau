
def Real foo(Int x):
    total = 1.
    i = 0
    while i < x:
        total += x
        total /= 2.
        i += 1
        end
    total
    end

print foo(1)
print foo(5)
print foo(10)

def Real bar(Real x):
    y = Real[50]
    i = 0
    while i < 50:
        y[i] = sin(i*x/50.)
        i += 1
        end
    y[49]
    free(y)
    end

print bar(2.)
